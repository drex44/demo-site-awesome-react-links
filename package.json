{
  "name": "react-links-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "awesome-react-links": "^1.0.0",
    "emotion": "^10.0.9",
    "node-fetch": "^2.2.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-markdown": "^4.0.8",
    "react-scripts": "2.1.8",
    "react-syntax-highlighter": "^10.2.1",
